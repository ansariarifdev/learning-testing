generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

enum RequestType {
  sum
  multiply
}

model Request {
  id     String      @id @default(uuid())
  type   RequestType
  a      Int
  b      Int
  result Int
}
